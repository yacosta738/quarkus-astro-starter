{
  "name": "Astro Wind",
  "description": "A template to make your website using Astro + Tailwind CSS.",
  "version": "0.3.5",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && cpx \"./dist/**/*\" \"../resources/META-INF/resources\"",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier -w .",
    "lint:eslint": "eslint . --ext .js,.ts,.astro",
    "subfont": "subfont -ir --no-fallbacks --silent --root dist"
  },
  "devDependencies": {
    "@astrojs/image": "0.7.1",
    "@astrojs/partytown": "1.0.0",
    "@astrojs/rss": "1.0.1",
    "@astrojs/sitemap": "1.0.0",
    "@astrojs/tailwind": "1.0.1",
    "@astrolib/analytics": "0.2.2",
    "@astrolib/seo": "0.2.1",
    "@fontsource/inter": "4.5.12",
    "@tailwindcss/typography": "0.5.7",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "astro": "1.3.0",
    "astro-icon": "0.7.3",
    "cpx": "1.5.0",
    "eslint": "8.23.1",
    "eslint-plugin-astro": "0.19.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "limax": "3.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.5.4",
    "reading-time": "1.5.0",
    "subfont": "6.12.1",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": "^14.15.0 || >=16.0.0"
  }
}
